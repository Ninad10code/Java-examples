class addition
{
    int add(int a, int b)
    {
         return a+b;
    }
    
    int add(double a, double b)
    {
         return a+b;
    }
}
public class calculation
{
   public static void main(String args[])
   {
	   addition obj = new addition();
       System.out.println(obj.add(10, 15));
       System.out.println(obj.add(10.5, 15.5));
   }
}
